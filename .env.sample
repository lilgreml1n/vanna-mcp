# SSH Tunnel Configuration
SSH_HOST=your-server.example.com
SSH_PORT=22
SSH_USERNAME=your-username
SSH_PASSWORD=your-password
# OR use SSH key (comment out SSH_PASSWORD if using key)
# SSH_KEY_PATH=/path/to/your/private/key
# SSH_KEY_PASSPHRASE=your-key-passphrase

# Remote MySQL Configuration (via SSH tunnel)
MYSQL_REMOTE_HOST=localhost
MYSQL_REMOTE_PORT=3306
MYSQL_DATABASE=your_database_name
MYSQL_USER=your_mysql_user
MYSQL_PASSWORD=your_mysql_password

# Vanna AI Configuration
VANNA_MODEL=your_model_name
VANNA_API_KEY=your_vanna_api_key
